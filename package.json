{
  "name": "package-starter",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "lint": "eslint src --ext .js,.ts --cache",
    "build": "pnpm clean && tsc",
    "clean": "rimraf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "devDependencies": {
    "@antfu/eslint-config": "1.0.0-beta.10",
    "chalk": "^5.3.0",
    "eslint": "^8.50.0",
    "lint-staged": "^13.3.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "src/**/*": "eslint"
  }
}
